#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/lib/node_modules:/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/node_modules:/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/fastapi@0.0.8/node_modules:/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/lib/node_modules:/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/fastapi@0.0.8/node_modules/fastapi/node_modules:/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/fastapi@0.0.8/node_modules:/mnt/e/MetaMinder/Learning/PromptTest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fastapi/lib/cli.js" "$@"
else
  exec node  "$basedir/../fastapi/lib/cli.js" "$@"
fi
